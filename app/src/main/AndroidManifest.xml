<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.albertcanales.poliedres">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/poliedres"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/poliedres"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.MenuActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.PrismaPiramideActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.CilindreActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.EsferaActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.ConoActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".activities.BaseActivity"></activity>
    </application>

</manifest>